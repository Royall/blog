<%- include header %>



<div class="reg_box">
<form method="post">




    <table border="0">
        <tr>

            <td>用户名：</td>
            <td> <input type="text" name="name"/></td>
        </tr>

        <tr>

            <td> 密码：</td>
            <td><input type="password" name="password"/></td>
        </tr>

        <tr>

            <td>确认密码：</td>
            <td> <input type="password" name="password-repeat"/></td>
        </tr>

        <tr>

            <td>邮箱：</td>
            <td><input type="email" name="email"/></td>
        </tr>


        <tr>

            <td>&nbsp;</td>
            <td><input type="submit" value="注册" class="reg_btn"/></td>
        </tr>


    </table>






</form>
</div>


<%- include footer %>